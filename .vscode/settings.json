{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "node_modules/": true,
    "build/": true,
    "dist/": true
  },
  "files.exclude": {
    "node_modules/": true,
    "build/": true,
    "dist/": true
  },
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true
}
